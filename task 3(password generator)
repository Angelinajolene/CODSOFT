import random
uppercase="ABCDEFGHIJKLMNOPQRSTUVWXYZ"
lowercase="abcdefghijklmnopqrstuvwxyz"
digits="0123456789"
specialchar="!@#$%^&*()?><:"
al=uppercase+lowercase+digits
password=int(input("enter the number of passwords to be generated"))
length= int(input("enter the length of the password"))
for i in range(password):
a="".join(random.sample(al,length))
print("thr password is:",a)
